//import isima.videoteca.modelo.negocio.Usuarios;
import isima.videoteca.modelo.negocio.*;

public interface VideotecaDAO {

	void registrarTema(Temas tema);
	void actualizarTema(Temas tema);
	void eliminarTema(Temas tema);
	Temas obtenerTemas(Integer clave);
	
	List<Temas>obtenerTemas();
	
	void registrarUsuario(Usuarios usuario);
	void actualizarUsuario(Usuarios usuario);
	void elimnarUsuario(Usuarios usuario);
	Usuarios obtenerUsuarios(Integer clave);
	
	List<Usuarios>obtenerUsuarios();
	
	void registrarPrestamos(Prestamos prestamos);
	void actualizarPrestamos(Prestamos prestamos);
	void eliminarPrestamos(Prestamos Prestamos);
	Prestamos obtenerPrestamos(Integer clave);
	
	List<Prestamos>obtenerPrestamos();
	
	void registrarProgramaEducativo(ProgramaEducativos programaEducativo);
	void actualizarProgramaEducativo(ProgramaEducativos programaEducativo);
	void eliminarProgramaEducativo(ProgramaEducativos programaEducativo);
	ProgramaEducativos obtenerProgramaEducativos(ProgramaEducativos programaEducativo);
	
	List<ProgramaEducativos>obtenerProgramaEducativo();
	
	}
