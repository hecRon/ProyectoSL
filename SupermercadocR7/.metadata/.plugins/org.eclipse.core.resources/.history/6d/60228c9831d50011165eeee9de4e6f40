import java.util.List;
//import isima.videoteca.modelo.negocio.Tema;
//import isima.videoteca.modelo.negocio.Usuario;
import isima.videoteca.modelo.negocio.*;

public interface VideotecaDAO {

	void registrarTema(Tema tema);
	void actualizarTema(Tema tema);
	void eliminarTema(Tema tema);
	void obtenerTema(Tema tema);
	
	List<Tema>obtenerTemas();
	
	void registrarUsuario(Usuario usuario);
	void actualizarUsuario(Usuario usuario);
	void elimnarUsuario(Usuario usuario);
	void obtenerUsuario(Usuario usuario);
	
	List<Usuario>obtenerUsuario();
	
	void registrarPrestamos(Prestamos prestamos);
	void actualizarPrestamos(Prestamos prestamos);
	void eliminarPrestamos(Prestamos prestamos);
	Prestamos void obtenerPrestamo(Integer clave);
	
	List<Prestamos>obtenerPrestamos();
	
	void registrarProgramaEducativo(ProgramaEducativo programaEducativo);
	void actualizarProgramaEducativo(ProgramaEducativo programaEducativo);
	void eliminarProgramaEducativo(ProgramaEducativo programaEducativo);
	void obtenerProgramaEducativo(ProgramaEducativo programaEducativo);
	
	List<ProgramaEducativo>obtenerProgramaEducativo();
	
	}
